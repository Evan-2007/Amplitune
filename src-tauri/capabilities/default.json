{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "os:default",
    "deep-link:default",
    {
      "identifier": "http:default",
      "allow": [
        "https://amp-api.music.apple.com",
        "https://*.apple.com",
        "https://www.youtube.com"
      ]
    }
  ]
}